<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>real_bringup</name>
  <version>0.0.1</version>
  <description>Package for starting up real xArm6 robot setup</description>
  <maintainer email="nermin.covic@etf.unsa.ba">root</maintainer>
  <maintainer email="dinko.osmankovic@etf.unsa.ba">root</maintainer>
  <license>TODO: License declaration</license>

  <buildtool_depend>ament_cmake</buildtool_depend>
  <buildtool_depend>ament_cmake_python</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_action</depend>
  <depend>rclpy</depend>
  <depend>trajectory_msgs</depend>
  <depend>control_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>visualization_msgs</depend>
  <depend>octomap_msgs</depend>
  <depend>octomap</depend>
  <depend>std_srvs</depend>
  <depend>librealsense2</depend>
  <depend>realsense2_camera</depend>
  <depend>perception_pcl</depend>
  <depend>pcl_conversions</depend>
  <depend>xarm_msgs</depend>
  <depend>xarm_api</depend>
  <depend>xarm_sdk</depend>
  <depend>rpmpl_library</depend>
  <depend>sim_bringup</depend>

  <exec_depend>ros2launch</exec_depend>
    
  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
